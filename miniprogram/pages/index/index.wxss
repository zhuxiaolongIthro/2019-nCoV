/**index.wxss**/

page {
  background: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: flex-start
}
.titleText{
  font-size: 28px
}
.list-title{
  padding-left: 8px;
  font-size: 24px
}
.list-item-layout{
  width: 100%;
  justify-content: space-between;
  align-items: baseline;
  margin-bottom: 8px;
  flex-direction: row;
}
.list-item{
  width: 25%;
  justify-content: space-between;
  text-align: center;
  display: inline-block;
  font-size: 14px
}
.line{
  height: 1px;
  margin-left: 8px;
  margin-right: 8px;
  background-color: gray
}


